    speaker: {fileID: 11400000, guid: 755db819ff1a2ea4aa12594b5b3997b8, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: 755db819ff1a2ea4aa12594b5b3997b8, type: 2}
      spriteIndex: 0
      actionType: 0
      coords: {x: 0, y: 0}
      moveSpeed: 0
  - text: "\uCCAB \uC190\uB2D8\uC740 \uC5B4\uB5A4 \uBD84\uC774 \uC624\uC2E4\uAE4C?"
    speaker: {fileID: 11400000, guid: 755db819ff1a2ea4aa12594b5b3997b8, type: 2}
